{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugin":[
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier":"error",
    "no-useless-constructor": "off"

  }
}
